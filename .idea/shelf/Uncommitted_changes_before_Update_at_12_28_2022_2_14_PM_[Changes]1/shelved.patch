+package edu.bluejack22_1.pillreminder.controller.treatment
+
+import android.content.Intent
+import android.os.Bundle
+
+import android.widget.ImageView
+import androidx.appcompat.app.AppCompatActivity
+import androidx.recyclerview.widget.LinearLayoutManager
+import androidx.recyclerview.widget.RecyclerView
+import edu.bluejack22_1.pillreminder.adapter.TreatAdapter
+import edu.bluejack22_1.pillreminder.databinding.FragmentTreatmentMainBinding
+import edu.bluejack22_1.pillreminder.model.Treatment
+
+class TreatMain: AppCompatActivity(), TreatAdapter.TreatClickListener {
+    private lateinit var binding: FragmentTreatmentMainBinding
+    private lateinit var rvTreat: RecyclerView
+    private lateinit var treatAdapter: TreatAdapter
+    private lateinit var toAddTreat: ImageView
+
+    override fun onCreate(savedInstanceState: Bundle?) {
+        binding = FragmentTreatmentMainBinding.inflate(layoutInflater)
+        toAddTreat = binding.toAddTreatment
+        toAddTreat.setOnClickListener {
+            startActivity(Intent(this, TreatmentAdd::class.java))
+        }
+        buildRecylcerView()
+        super.onCreate(savedInstanceState)
+        setContentView(binding.root)
+    }
+
+    override fun onResume() {
+        super.onResume()
+        buildRecylcerView()
+    }
+
+    private fun buildRecylcerView(){
+        rvTreat = binding.rvTreat
+        rvTreat.layoutManager = LinearLayoutManager(this, LinearLayoutManager.VERTICAL, false)
+        treatAdapter = TreatAdapter(Treatment.treatmentList, this)
+        rvTreat.adapter = treatAdapter
+    }
+
+    override fun onTreatClicked(pos: Int) {
+        var intent = Intent(this, TreatmentDetail::class.java)
+        intent.putExtra("treatment", Treatment.treatmentList.get(pos))
+        startActivity(intent)
+    }
+
+
+}
