+package edu.bluejack22_1.pillreminder.controller.treatment
+
+import android.app.DatePickerDialog
+import android.os.Bundle
+import android.os.PersistableBundle
+import android.widget.Button
+import android.widget.EditText
+import android.widget.ImageView
+import android.widget.TextView
+import androidx.appcompat.app.AppCompatActivity
+import edu.bluejack22_1.pillreminder.R
+import edu.bluejack22_1.pillreminder.databinding.ActivityTreatmentAddBinding
+import java.text.SimpleDateFormat
+import java.util.*
+
+class TreatmentAdd: AppCompatActivity() {
+    private lateinit var binding: ActivityTreatmentAddBinding
+    private lateinit var backButton: ImageView
+    private lateinit var addTreatName: EditText
+    private lateinit var addTreatUnit: EditText
+    private lateinit var addTreatUnit2: String
+//    private lateinit var addTreatStartDate: EditText
+//    private lateinit var addTreatEndDate: EditText
+    private lateinit var addTreatFreq: EditText
+    private lateinit var addTreatRemind: EditText
+    private lateinit var toAddTreat: Button
+
+    private lateinit var tvStartDate: TextView
+    private lateinit var tvEndDate: TextView
+    private lateinit var StartDateBtn: Button
+    private lateinit var EndDateBtn: Button
+
+
+    private lateinit var treatNm: String
+    private lateinit var treatUnit: String
+    private lateinit var treatUnit2: String
+    private lateinit var treatStart: String
+    private lateinit var treatEnd: String
+    private lateinit var treatFreq: String
+    private lateinit var treatRmd: String
+
+
+    override fun onCreate(savedInstanceState: Bundle?) {
+        binding = ActivityTreatmentAddBinding.inflate(layoutInflater)
+        backButton = binding.backButton
+        backButton.setOnClickListener { finish() }
+        addTreatName = binding.addTreatName
+        addTreatUnit = binding.addTreatUnit
+        addTreatUnit2 = binding.addTreatUnit2.selectedItem.toString()
+        addTreatFreq = binding.addTreatFreq
+        addTreatRemind = binding.addTreatRemind
+//        addTreatStartDate = binding.addTreatStartDate
+//        addTreatEndDate = binding.addTreatEndDate
+//        toAddDoc = binding.toAddDoc
+
+        tvStartDate = findViewById(R.id.tvStartDt)
+        StartDateBtn = findViewById(R.id.StartDateBtn)
+
+        tvEndDate = findViewById(R.id.tvEndDt)
+        EndDateBtn = findViewById(R.id.EndDateBtn)
+
+        val myCalendar = Calendar.getInstance()
+
+        val datePicker = DatePickerDialog.OnDateSetListener{ view, year, month, dayOfMonth ->
+            myCalendar.set(Calendar.YEAR, year)
+            myCalendar.set(Calendar.MONTH, month)
+            myCalendar.set(Calendar.DAY_OF_MONTH, dayOfMonth)
+            updateLable(myCalendar)
+        }
+
+        StartDateBtn.setOnClickListener {
+            DatePickerDialog(this, datePicker, myCalendar.get(Calendar.YEAR), myCalendar.get(Calendar.MONTH), myCalendar.get(Calendar.DAY_OF_MONTH)).show()
+
+        }
+
+        super.onCreate(savedInstanceState)
+        setContentView(binding.root)
+    }
+
+    private fun updateLable( myCalendar: Calendar){
+        val myFormat = "dd-MM-yyyy"
+        val sdf = SimpleDateFormat(myFormat, Locale.UK)
+        tvStartDate.setText(sdf.format(myCalendar.time))
+
+
+    }
+
+
+
+    private fun frontVal(): String{
+        treatNm = addTreatName.text.toString()
+        treatUnit = addTreatUnit.text.toString()
+        treatUnit2 = addTreatUnit2
+        treatFreq = addTreatFreq.text.toString()
+        treatRmd = addTreatRemind.text.toString()
+//        treatStart = addTreatStartDate.text.toString()
+//        treatEnd = addTreatEndDate.text.toString()
+
+//        if()
+
+
+        return ""
+
+    }
+
+}
