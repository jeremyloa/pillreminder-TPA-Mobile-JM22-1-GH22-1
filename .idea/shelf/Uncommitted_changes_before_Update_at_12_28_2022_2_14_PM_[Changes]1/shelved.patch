+package edu.bluejack22_1.pillreminder.model
+
+import android.util.Log
+import com.google.firebase.firestore.ktx.firestore
+import com.google.firebase.ktx.Firebase
+import java.io.Serializable
+import java.util.*
+
+class Treatment(val documentid:String?, val patientid:String?, var name:String?, var dose: String, var startdate:Date, var enddate:Date, var frequency:String?, var remindtime:String, var type:String, var unit: Long): Serializable {
+    companion object{
+        var db = Firebase.firestore
+        var treatmentList: MutableList<Treatment> = mutableListOf()
+
+        fun fetch_all_treatments(){
+            treatmentList.clear()
+            db.collection("treatments")
+                .whereEqualTo("patientid", User.curr.uid)
+                .get()
+                .addOnSuccessListener { trs ->
+                    for (tr in trs){
+                        val temp =
+                            Treatment(
+                                tr.id,
+                                tr.data.get("dose").toString(),
+                                tr.data.get("patientid").toString(),
+                                tr.data.get("dose").toString(),
+                                Date(tr.data.get("startdate").toString().toLong()),
+                                Date(tr.data.get("enddate").toString().toLong()),
+                                tr.data.get("frequency").toString(),
+                                tr.data.get("remindtime").toString(),
+                                tr.data.get("type").toString(),
+                                tr.data.get("unit").toString().toLong()
+                            )
+                        val tempIndex = treatmentList.indexOf(temp)
+                        if (tempIndex<0 || tempIndex > treatmentList.size){
+                            treatmentList.add(temp)
+                            Log.i("GET_TREATMENTS", "Document: " + temp.documentid + " Place " + temp.name.toString())
+                        }
+
+                    }
+                }
+                .addOnFailureListener{ e ->
+                    Log.e("GET_TREATMENTS", e.toString())
+                }
+        }
+
+
+
+    }
+}
